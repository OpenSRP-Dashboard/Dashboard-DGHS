app/script/app.js 


.config(function ($routeProvider,$locationProvider) {
    $routeProvider
      .when('/camp', {
        templateUrl: 'views/camp/index.html',
        controller: 'CampCtrl',
        controllerAs: 'camp'        
      })
  })
